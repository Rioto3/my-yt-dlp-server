{
  "manifest_version": 2,
  "name": "YouTube Audio Extractor",
  "version": "1.0",
  "description": "Extract audio from YouTube videos",
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}